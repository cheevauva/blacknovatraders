{% extends "layout.twig" %}
{% block title %}{{ tr.l_log_titlet }}{% endblock %}
{% block content %}

    {% for log in logs %}
        <table>
            <tr> 
                <td>{{ log.title()|raw }}</td> 
                <td>{{ log.time() }}</td> 
            </tr>
            <tr>
                <td colspan=2> 
                    {{ log.text()|raw }}
                </td>
            </tr> 
        </table> 
        <hr>
    {% endfor %}

{% endblock %}

